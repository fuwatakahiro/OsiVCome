<%= form_with model: @character do |f| %>
  <%= f.file_field :character_image, accept: "image/*" %>
  <%= f.label :名前 %>
  <%= f.text_field :name %>
  <%= f.label :挨拶 %>
  <%= f.text_field :greeting %>
  <%= f.label :ジャンル %>
  <%= f.text_field :genre %>
  <%= f.label :引用先 %>
  <%= f.text_area :reference_destination %>
  <%= f.submit "新規登録" %>
<% end %>
  
