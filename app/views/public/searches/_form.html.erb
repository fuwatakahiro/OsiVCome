<%= form_with url: search_path, method: :get do |f| %>
  <%= f.text_field :content, placeholder: "Search" %>
  <%= f.select :model, options_for_select({"Customer"=> "customer", "Character" => "character"}), include_blank: "検索選択" %>
  <%= button_tag type: "submit", class: "btn rounded-0 border-0 input-group-btn" do %>
    <i class="fa fa-search"></i>
  <% end %>
<% end %>