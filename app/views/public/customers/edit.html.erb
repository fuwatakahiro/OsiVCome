<%= render 'layouts/errors', obj: @customer %>
<%= form_with model: @customer, url: customer_path, method: :patch do |f| %>
  <%= f.label :画像 %>
  <%= f.file_field :profile_image, accept: "image/*" %><br>
  <%= f.label :名前 %>
  <%= f.text_field :name %><br>
  <%= f.hidden_field :email %>
  <%= f.submit "更新" %>
<% end %>